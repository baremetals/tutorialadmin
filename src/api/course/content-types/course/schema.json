{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "duration": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "notes": {
      "type": "richtext"
    },
    "githubLink": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "startDate": {
      "type": "datetime",
      "required": false
    },
    "endDate": {
      "type": "datetime",
      "required": false
    },
    "totalStudents": {
      "type": "integer",
      "default": 0
    },
    "totalLessons": {
      "type": "integer",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "teacher": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::teacher.teacher"
    },
    "videos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-video.course-video"
    },
    "students": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student.student"
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "introduction": {
      "type": "text"
    },
    "courseType": {
      "type": "enumeration",
      "enum": [
        "group",
        "single"
      ],
      "required": false,
      "default": "group"
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "Beginner",
        "Intermediate",
        "Advance"
      ],
      "required": true
    },
    "soloPrice": {
      "type": "decimal",
      "required": false,
      "default": 0
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "isFree": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "course"
    }
  }
}
