{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "duration": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "notes": {
      "type": "richtext"
    },
    "githubLink": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "startDate": {
      "type": "datetime",
      "required": true
    },
    "endDate": {
      "type": "datetime",
      "required": true
    },
    "totalStudents": {
      "type": "integer",
      "default": 0
    },
    "totalLessons": {
      "type": "integer",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "teacher": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::teacher.teacher"
    },
    "course_videos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-video.course-video"
    },
    "students": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student.student"
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    }
  }
}
