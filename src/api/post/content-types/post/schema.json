{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "isDisabled": {
      "type": "boolean",
      "default": false
    },
    "views": {
      "type": "integer",
      "default": 0
    },
    "points": {
      "type": "integer",
      "default": 0
    },
    "body": {
      "type": "richtext"
    },
    "creator": {
      "type": "string",
      "required": true
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "post_points": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post-point.post-point"
    }
  }
}
