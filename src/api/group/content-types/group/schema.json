{
  "kind": "collectionType",
  "collectionName": "groups",
  "info": {
    "singularName": "group",
    "pluralName": "groups",
    "displayName": "Group",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course.course"
    },
    "name": {
      "type": "string"
    },
    "students": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "lectures": {
      "type": "component",
      "repeatable": true,
      "component": "lectures.lectures"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "startTime": {
      "type": "time"
    },
    "endTime": {
      "type": "time"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::group-message.group-message"
    },
    "qna": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question-and-answer.question-and-answer"
    }
  }
}
